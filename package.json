{
  "name": "@solido/atlante-js",
  "version": "0.1.11",
  "description": "JS Http Client",
  "license": "MIT",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "coverage": "nyc npm test",
    "coverage-html": "nyc --reporter=html npm test",
    "lint": "eslint lib/",
    "test": "node ./tests.js \"test/**/*.js\""
  },
  "author": "Alessandro Chitolina <alekitto@gmail.com>",
  "dependencies": {
    "@jymfony/util": "^0.1.0-alpha.25",
    "@types/chai": "^4.2.11"
  },
  "devDependencies": {
    "@jymfony/autoloader": "^0.1.0-alpha.25",
    "@jymfony/compiler": "^0.2.2",
    "@jymfony/datetime": "^0.1.0-alpha.25",
    "@jymfony/debug": "^0.1.0-alpha.25",
    "@jymfony/testing": "^0.1.0-alpha.25",
    "@jymfony/var-dumper": "^0.1.0-alpha.25",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "chai": "^4.2.0",
    "eslint": "^7.5.0",
    "mocha": "^6.2.0",
    "node-localstorage": "^1.3.1",
    "nyc": "^14.1.1",
    "typescript": "^3.9.7",
    "whatwg-url": "^7.0.0"
  },
  "config": {
    "jymfony-autoload": {
      "namespaces": {
        "Solido.Atlante": "lib"
      }
    }
  },
  "nyc": {
    "per-file": true,
    "hookRequire": true,
    "hookRunInContext": true,
    "hookRunInThisContext": true,
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "**/tests.js",
      "**/namespace-stub.js",
      "**/fixtures/**",
      "**/test/**"
    ],
    "reporter": [
      "text-summary",
      "text",
      "clover"
    ],
    "all": true,
    "report-dir": "./coverage"
  }
}
